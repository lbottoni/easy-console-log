{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "ignorePatterns": ["**/*.md"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-new": "off",
        "import/extensions": "off"
    }
}
